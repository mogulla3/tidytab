{
  "manifest_version": 3,
  "name": "tidytab",
  "description": "Tidy up tabs on your browser",
  "version": "0.0.1",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "tabGroups"
  ]
}
